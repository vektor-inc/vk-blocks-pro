{
	"apiVersion": 3,
	"name": "vk-blocks/table-of-contents-new",
	"title": "Table of Contents",
	"description": "This is a table of contents that is automatically generated according to the headings when added.",
	"textdomain": "vk-blocks-pro",
	"category": "vk-blocks-cat",
	"editorScript": "vk-blocks-build-js",
	"editorStyle": "vk-blocks-build-editor-css",
	"attributes": {
		"style": {
			"type": "string",
			"default": "default"
		},
		"renderHtml": {
			"type": "string",
			"default": ""
		},
		"open": {
			"type": "string",
			"default": "open"
		},
		"className": {
			"type": "string",
			"default": ""
		},
		"customHeadingLevels": {
			"type": "array",
			"default": []
		},
		"useCustomLevels": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"className": true
	}
}
