{
	"apiVersion": 2,
	"name": "vk-blocks/blog-card",
	"category": "vk-blocks-cat",
	"title": "Blog Card",
	"description": "Add a block that fetches and displays content from a URL.",
	"attributes": {
		"url": {
			"type": "string",
			"default": ""
		}
	},
	"providesContext": {
		"vk-blocks/blog-card-url": "url"
	},
	"textdomain": "vk-blocks-pro",
	"supports": {
		"align": ["wide", "full"],
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"layout": {
			"allowSwitching": false,
			"allowInheriting": false,
			"allowEditing": false
		},
		"html": false,
		"vkBlocksBlockVariation": {
			"excludeAttributes": ["url"],
			"scope": ["inserter", "block"]
		}
	}
}
