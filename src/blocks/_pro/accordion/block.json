{
	"apiVersion": 2,
	"name": "vk-blocks/accordion",
	"title": "Accordion",
	"description": "Collapses and hides content when the content is long.",
	"textdomain": "vk-blocks-pro",
	"category": "vk-blocks-cat",
	"editorScript": "vk-blocks-build-js",
	"editorStyle": "vk-blocks-build-editor-css",
	"style": "vk-blocks/accordion",
	"attributes": {
		"containerClass": {
			"type": "string",
			"default": "vk_accordion"
		},
		"initialState": {
			"type": "string",
			"default": "closed"
		},
		"initialStateMobile": {
			"type": "string",
			"default": "closed"
		},
		"initialStateTablet": {
			"type": "string",
			"default": "closed"
		},
		"initialStateDesktop": {
			"type": "string",
			"default": "closed"
		},
        "isDeviceSpecific": {
            "type": "boolean",
            "default": false
        }
	},
	"supports": {
		"html": false,
		"anchor": true,
		"className": true
	}
}
