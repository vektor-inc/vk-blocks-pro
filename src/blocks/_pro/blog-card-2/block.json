{
	"apiVersion": 2,
	"name": "vk-blocks/blog-card-2",
	"category": "vk-blocks-cat",
	"title": "Blog Card 2",
	"description": "URLからコンテンツを取り込んで表示するブロックを追加します。",
	"attributes": {
		"url": {
			"type": "string",
			"default": ""
		},
		"cardType": {
			"type": "string",
			"default": ""
		},
		"isLink": {
			"type": "boolean",
			"default": false
		},
		"rel": {
			"type": "string",
			"attribute": "rel",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"textdomain": "vk-blocks-pro",
	"supports": {
		"align": [ "wide", "full" ],
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"html": false
	}
}
