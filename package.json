{
  "name": "vk-block",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "start": "wp-env start",
    "stop": "wp-env stop",
    "destroy": "wp-env destroy",
    "watch": "gulp watch",
    "build:dev": "wp-scripts build --config webpack.config.js --mode=development && gulp build:dev:pro",
    "build": "wp-scripts build --config webpack.config.js && gulp build:pro && npm run translate",
    "build:free": "wp-scripts build --config webpack.config.js && gulp build:free && npm run translate",
    "translate": "./node_modules/.bin/po2json ./inc/vk-blocks/languages/vk-blocks-ja.po ./inc/vk-blocks/languages/vk-blocks-ja-vk-blocks-build-js.json -f jed",
    "gulp": "gulp",
    "lint:js": "wp-scripts lint-js",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style",
		"lint:alert": "wp-scripts format-js src/blocks/alert && wp-scripts lint-js src/blocks/alert --fix",
		"lint:pr-content": "wp-scripts format-js src/blocks/pr-content/ && wp-scripts lint-js src/blocks/pr-content/ --fix",
    "fixtures:clean": "rimraf \"packages/e2e-tests/fixtures/blocks/*.+(json|serialized.html)\"",
    "fixtures:generate": "cross-env GENERATE_MISSING_FIXTURES=y npm run test-unit",
    "fixtures:regenerate": "npm run fixtures:clean && npm run fixtures:generate",
    "test-unit": "wp-scripts test-unit-js --config test/jest.config.js test/integration/full-content/full-content.test.js",
    "composer:install:win": "wp-env run composer \"composer install\"",
    "composer:install:mac": "wp-env run composer 'composer install'",
    "phpunit:win": "wp-env run phpunit \"phpunit -c /var/www/html/wp-content/plugins/vk-blocks-pro/phpunit.xml.dist --verbose\"",
    "phpunit:mac": "wp-env run phpunit 'phpunit -c /var/www/html/wp-content/plugins/vk-blocks-pro/phpunit.xml.dist --verbose'"
  },
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    "> 5%"
  ],
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@emotion/babel-plugin": "^11.1.2",
    "@wordpress/babel-plugin-makepot": "^3.9.0",
    "@wordpress/block-editor": "^5.1.5",
    "@wordpress/block-library": "^2.26.6",
    "@wordpress/block-serialization-default-parser": "^3.8.0",
    "@wordpress/blocks": "^6.24.2",
    "@wordpress/env": "^2.1.0",
    "@wordpress/scripts": "^12.5.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-jsmin": "^0.1.5",
    "gulp-load-plugins": "^2.0.5",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^10.2.2",
    "po2json": "^1.0.0-beta-3",
    "prettier": "^2.2.1",
    "url-loader": "^4.1.1"
  },
  "dependencies": {
    "@wordpress/components": "^11.1.1",
    "@wordpress/data": "^4.25.0",
    "@wordpress/edit-post": "^3.25.4",
    "@wordpress/element": "^2.18.0",
    "@wordpress/hooks": "^2.10.0",
    "@wordpress/i18n": "^3.16.0",
    "@wordpress/icons": "^2.8.0",
    "@wordpress/list-reusable-blocks": "^1.24.1",
    "@wordpress/plugins": "^2.23.0",
    "@wordpress/viewport": "^2.24.0",
    "compare-versions": "^3.6.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-html-parser": "^2.0.2",
    "memize": "^1.1.0",
    "classnames": "^2.2.6"
  }
}
