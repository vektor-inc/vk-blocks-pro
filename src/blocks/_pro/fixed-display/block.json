			"default": 0
		},
		"scrollTimingUnit": {
			"type": "string",
			"default": "px"
		},
		"clientId": {
			"type": "string"
		},
		"blockId": {
			"type": "string"
		}
	},
	"description": "Remains fixed on the screen at all times.",
	"textdomain": "vk-blocks-pro",
	"supports": {
		"html": false,
		"className": true,
		"align": ["wide", "full", "left", "right", "center"]
	}
}
