{
	"apiVersion": 2,
	"name": "vk-blocks/blog-card-site-logo",
	"category": "vk-blocks-cat",
	"title": "Blog Card Site Logo",
	"description": "URLから取得したサイトロゴ画像を表示します。",
	"textdomain": "vk-blocks-pro",
	"ancestor": [ "vk-blocks/blog-card" ],
	"attributes": {
		"isLink": {
			"type": "boolean",
			"default": true
		},
		"rel": {
			"type": "string",
			"attribute": "rel",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"usesContext": [
		"vk-blocks/blog-card-url"
	],
	"supports": {
		"align": [ "left", "right", "center", "wide", "full" ],
		"color": {
			"__experimentalDuotone": "img",
			"text": false,
			"background": false
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"selectors": "img",
			"__experimentalSkipSerialization": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"width": true
			}
		},
		"html": false,
		"reusable": false,
		"spacing": {
			"margin": true
		}
	}
}
