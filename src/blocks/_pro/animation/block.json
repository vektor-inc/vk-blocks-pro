{
	"apiVersion": 2,
	"name": "vk-blocks/animation",
	"category": "vk-blocks-cat",
	"title": "Animation",
	"editorScript": "vk-blocks-build-js",
	"editorStyle": "vk-blocks-build-editor-css",
	"style": "vk-blocks-build-css",
	"attributes": {
		"effect": {
			"type": "string",
			"default": "slide-up"
		},
		"speed": {
			"type": "string",
			"default": "fast"
		},
		"range": {
			"type": "string",
			"default": "short"
		},
		"once": {
			"type": "boolean",
			"default": false
		},
		"clientId": {
			"type": "string"
		},
		"blockId": {
			"type": "string"
		}
	},
	"description": "Add animation to elements when scrolling the page.",
	"textdomain": "vk-blocks-pro",
	"supports": {
		"html": false,
		"className": true
	}
}
