{
	"apiVersion": 2,
	"name": "vk-blocks/blog-card-site-title",
	"category": "vk-blocks-cat",
	"title": "Blog Card Site Title",
	"description": "URLから取得したサイトタイトルを表示します。",
	"textdomain": "vk-blocks-pro",
	"ancestor": [ "vk-blocks/blog-card" ],
	"attributes": {
		"level": {
			"type": "number",
			"default": 0
		},
		"textAlign": {
			"type": "string"
		},
		"isLink": {
			"type": "boolean",
			"default": true
		},
		"rel": {
			"type": "string",
			"attribute": "rel",
			"default": ""
		},
		"linkTarget": {
			"type": "string",
			"default": "_self"
		}
	},
	"usesContext": [
		"vk-blocks/blog-card-url"
	],
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"spacing": {
			"padding": true
		}
	}
}
